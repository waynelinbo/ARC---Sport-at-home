ifeq ($(VALID_BD_VER), rev2)

ifeq ($(VALID_CUR_CORE), em7d_esp_dfss)
LIB_SECURESHIELD_VERSION = 2
else

ifeq ($(VALID_CUR_CORE), em11d_dfss)
LIB_SECURESHIELD_VERSION = 1
else
$(error $(VALID_BOARD)-$(VALID_BD_VER)-$(VALID_CUR_CORE) is not supported in secureshield!)
endif
endif

else
$(error $(VALID_BOARD)-$(VALID_BD_VER)-$(VALID_CUR_CORE) is not supported in secureshield!)
endif


LIB_SECURESHIELD_BOARD_EMSDP_DIR = $(LIB_SECURESHIELD_DIR)/board/emsdp

LIB_SECURESHIELD_ASMSRCDIR += $(LIB_SECURESHIELD_BOARD_EMSDP_DIR)/$(VALID_BD_VER)
LIB_SECURESHIELD_CSRCDIR += $(LIB_SECURESHIELD_BOARD_EMSDP_DIR)/common $(LIB_SECURESHIELD_BOARD_EMSDP_DIR)/$(VALID_BD_VER)
LIB_SECURESHIELD_INCDIR += $(LIB_SECURESHIELD_BOARD_EMSDP_DIR)/$(VALID_BD_VER)

SECURESHIELD_BOARD_CONFIG_FILE = $(LIB_SECURESHIELD_BOARD_EMSDP_DIR)/$(VALID_BD_VER)/secureshield_board_config.h
